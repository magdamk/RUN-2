<br><br>
<input type="button" class="btn btn-outline-primary" onclick="printDiv()" value="Wydrukuj listę startową" />
<br><br>
<div id="doDruku">
<h2>Lista startowa Bieg {{race}}</h2>
<table class="table table-hover table-striped">
    <thead class="table-dark">
        <tr>
            <th>Numer</th>
            <th>Nazwisko</th>
            <th>Imię</th>
            <th>Klub</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{this.start_id}}</td> 
            <td>{{this.l_name}}</td>
            <td>{{this.f_name}} </td>
            <td>{{this.club}} </td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div>

